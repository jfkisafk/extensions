{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "databricks",
  "title": "databricks",
  "description": "Extension to interact with your databricks account and workspaces",
  "icon": "extension-icon.png",
  "author": "stelo",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "delta-live-tables",
      "title": "Delta Live Tables",
      "description": "Manage Delta Live Table pipelines for your workspaces",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.80.0",
    "@raycast/utils": "^1.16.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "@raycast/eslint-config": "^1.0.8",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
